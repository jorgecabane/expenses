{
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true"
  },
  "crons": [
    {
      "path": "/api/cron/recurring-transactions",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/reset-payment-tasks",
      "schedule": "0 0 1 * *"
    }
  ]
}
